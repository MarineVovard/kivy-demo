<Touchtracer>:
    canvas:
        Color:
            rgb: 0, 0, 0
        Rectangle:
            size: self.size
            pos: self.pos

<TouchTracerScreen>:
    PhytecBackground: 
        SmallerWhiteBackground: 
            id: template_background

            BoxLayout:
                orientation:"horizontal"
                size_hint: 1,0.05

                TextLabel: 
                    id: title
                    text: "[size=20sp]Multitouch[/size]"
                
                Button:
                    text: "Home"
                    size_hint_x: 0.1
                    on_release: root.manager.current='home'

            Touchtracer:
                id: touchTracer
                # pos: self.pos